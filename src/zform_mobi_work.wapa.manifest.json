{                                                                                                                                                                                                                                                              
	"_version": "1.12.0",                                                                                                                                                                                                                                         
	"sap.app": {                                                                                                                                                                                                                                                  
		"id": "com.steb.mobileWork",                                                                                                                                                                                                                                 
		"type": "application",                                                                                                                                                                                                                                       
		"i18n": "i18n/i18n.properties",                                                                                                                                                                                                                              
		"title": "{{appTitle}}",                                                                                                                                                                                                                                     
		"description": "{{appDescription}}",                                                                                                                                                                                                                         
		"applicationVersion": {                                                                                                                                                                                                                                      
			"version": "1.0.0"                                                                                                                                                                                                                                          
		},                                                                                                                                                                                                                                                           
		"dataSources": {                                                                                                                                                                                                                                             
			"main": {                                                                                                                                                                                                                                                   
				"uri": "/sap/opu/odata/sap/ZFORM_MOBILE_WORK_SRV",                                                                                                                                                                                                         
				"type": "OData",                                                                                                                                                                                                                                           
				"settings": {                                                                                                                                                                                                                                              
					"odataVersion": "2.0"                                                                                                                                                                                                                                     
				}                                                                                                                                                                                                                                                          
			},                                                                                                                                                                                                                                                          
			"shared": {                                                                                                                                                                                                                                                 
				"uri": "/sap/opu/odata/sap/ZFORM_SHARED_SRV",                                                                                                                                                                                                              
				"type": "OData",                                                                                                                                                                                                                                           
				"settings": {                                                                                                                                                                                                                                              
					"odataVersion": "2.0"                                                                                                                                                                                                                                     
				}                                                                                                                                                                                                                                                          
			}                                                                                                                                                                                                                                                           
		}                                                                                                                                                                                                                                                            
	},                                                                                                                                                                                                                                                            
	"sap.ui": {                                                                                                                                                                                                                                                   
		"technology": "UI5",                                                                                                                                                                                                                                         
		"icons": {},                                                                                                                                                                                                                                                 
		"deviceTypes": {                                                                                                                                                                                                                                             
			"desktop": true,                                                                                                                                                                                                                                            
			"tablet": true,                                                                                                                                                                                                                                             
			"phone": true                                                                                                                                                                                                                                               
		}                                                                                                                                                                                                                                                            
	},                                                                                                                                                                                                                                                            
	"sap.ui5": {                                                                                                                                                                                                                                                  
		"rootView": {                                                                                                                                                                                                                                                
			"viewName": "com.steb.mobileWork.view.App",                                                                                                                                                                                                                 
			"type": "XML",                                                                                                                                                                                                                                              
			"async": true,                                                                                                                                                                                                                                              
			"id": "app"                                                                                                                                                                                                                                                 
		},                                                                                                                                                                                                                                                           
		"dependencies": {                                                                                                                                                                                                                                            
			"minUI5Version": "1.120.0",                                                                                                                                                                                                                                 
			"libs": {                                                                                                                                                                                                                                                   
				"sap.ui.core": {},                                                                                                                                                                                                                                         
				"sap.m": {}                                                                                                                                                                                                                                                
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
		"handleValidation": true,                                                                                                                                                                                                                                    
		"contentDensities": {                                                                                                                                                                                                                                        
			"compact": true,                                                                                                                                                                                                                                            
			"cozy": true                                                                                                                                                                                                                                                
		},                                                                                                                                                                                                                                                           
		"models": {                                                                                                                                                                                                                                                  
			"main": {                                                                                                                                                                                                                                                   
				"dataSource": "main",                                                                                                                                                                                                                                      
				"type": "sap.ui.model.odata.v2.ODataModel",                                                                                                                                                                                                                
				"preload": false,                                                                                                                                                                                                                                          
				"settings": {                                                                                                                                                                                                                                              
					"defaultBindingMode": "TwoWay"                                                                                                                                                                                                                            
				}                                                                                                                                                                                                                                                          
			},                                                                                                                                                                                                                                                          
			"shared": {                                                                                                                                                                                                                                                 
				"dataSource": "shared",                                                                                                                                                                                                                                    
				"type": "sap.ui.model.odata.v2.ODataModel",                                                                                                                                                                                                                
				"preload": false,                                                                                                                                                                                                                                          
				"settings": {                                                                                                                                                                                                                                              
					"defaultBindingMode": "OneWay"                                                                                                                                                                                                                            
				}                                                                                                                                                                                                                                                          
			},                                                                                                                                                                                                                                                          
			"i18n": {                                                                                                                                                                                                                                                   
				"type": "sap.ui.model.resource.ResourceModel",                                                                                                                                                                                                             
				"settings": {                                                                                                                                                                                                                                              
					"bundleName": "com.steb.mobileWork.i18n.i18n"                                                                                                                                                                                                             
				}                                                                                                                                                                                                                                                          
			}                                                                                                                                                                                                                                                           
		},                                                                                                                                                                                                                                                           
		"routing": {                                                                                                                                                                                                                                                 
			"config": {                                                                                                                                                                                                                                                 
				"routerClass": "sap.m.routing.Router",                                                                                                                                                                                                                     
				"viewType": "XML",                                                                                                                                                                                                                                         
				"viewPath": "com.steb.mobileWork.view",                                                                                                                                                                                                                    
				"controlId": "app",                                                                                                                                                                                                                                        
				"controlAggregation": "pages",                                                                                                                                                                                                                             
				"async": true                                                                                                                                                                                                                                              
			},                                                                                                                                                                                                                                                          
			"routes": [                                                                                                                                                                                                                                                 
				{                                                                                                                                                                                                                                                          
					"pattern": ":?query:",                                                                                                                                                                                                                                    
					"name": "main",                                                                                                                                                                                                                                           
					"target": "main"                                                                                                                                                                                                                                          
				},                                                                                                                                                                                                                                                         
				{                                                                                                                                                                                                                                                          
					"pattern": "display{?query}",                                                                                                                                                                                                                             
					"name": "display",                                                                                                                                                                                                                                        
					"target": "display"                                                                                                                                                                                                                                       
				},                                                                                                                                                                                                                                                         
				{                                                                                                                                                                                                                                                          
					"pattern": "create",                                                                                                                                                                                                                                      
					"name": "create",                                                                                                                                                                                                                                         
					"target": "create"                                                                                                                                                                                                                                        
				},                                                                                                                                                                                                                                                         
				{                                                                                                                                                                                                                                                          
					"pattern": "detail/{scenarioid}/{wfInstanceId}/{taskPath}",                                                                                                                                                                                               
					"name": "detail",                                                                                                                                                                                                                                         
					"target": "detail"                                                                                                                                                                                                                                        
				}                                                                                                                                                                                                                                                          
			],                                                                                                                                                                                                                                                          
			"targets": {                                                                                                                                                                                                                                                
				"main": {                                                                                                                                                                                                                                                  
					"viewId": "overview",                                                                                                                                                                                                                                     
					"viewName": "Overview"                                                                                                                                                                                                                                    
				},                                                                                                                                                                                                                                                         
				"display": {                                                                                                                                                                                                                                               
					"viewId": "main",                                                                                                                                                                                                                                         
					"viewName": "Main"                                                                                                                                                                                                                                        
				},                                                                                                                                                                                                                                                         
				"create": {                                                                                                                                                                                                                                                
					"viewId": "main",                                                                                                                                                                                                                                         
					"viewName": "Main"                                                                                                                                                                                                                                        
				},                                                                                                                                                                                                                                                         
				"detail": {                                                                                                                                                                                                                                                
					"viewType": "XML",                                                                                                                                                                                                                                        
					"transition": "slide",                                                                                                                                                                                                                                    
					"viewLevel": 1,                                                                                                                                                                                                                                           
					"viwId": "main",                                                                                                                                                                                                                                          
					"viewName": "Main"                                                                                                                                                                                                                                        
				}                                                                                                                                                                                                                                                          
			}                                                                                                                                                                                                                                                           
		}                                                                                                                                                                                                                                                            
	}                                                                                                                                                                                                                                                             
}                                                                                                                                                                                                                                                              